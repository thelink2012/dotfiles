# Mute
"amixer set Master toggle; amixer set Speaker unmute;"
    XF86AudioMute

# Increase volume
"amixer set Master 1%+"
   XF86AudioRaiseVolume

# Decrease volume
"amixer set Master 1%-"
   XF86AudioLowerVolume

# Play/Pause
"playerctl play-pause"
    XF86AudioPlay

# Print Scr.
"takeshot"
    m:0x0 + c:107
    Print

